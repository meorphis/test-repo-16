{
  "name": "@mixedbread/cli",
  "version": "0.1.0",
  "description": "CLI tool for managing Mixedbread vector stores and files",
  "author": "Mixedbread <support@mixedbread.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "github:mixedbread-ai/mixedbread-ts",
    "directory": "packages/cli"
  },
  "keywords": [
    "mixedbread",
    "cli",
    "vector-store",
    "embeddings",
    "command-line"
  ],
  "bin": {
    "mxbai": "./dist/bin/mxbai.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
