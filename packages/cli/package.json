    "lint": "eslint src",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "@mixedbread/sdk": "^0.6.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "ora": "^8.0.1",
    "cli-table3": "^0.6.5",
    "inquirer": "^9.2.23",
    "glob": "^10.4.5",
    "yaml": "^2.4.5",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "@types/inquirer": "^9.0.7",
    "typescript": "5.8.3",
    "jest": "^29.4.0",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.4.0",
    "eslint": "^9.20.1",
    "typescript-eslint": "8.31.1",
    "@typescript-eslint/eslint-plugin": "8.31.1",
    "@typescript-eslint/parser": "8.31.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.0.0"
  }
